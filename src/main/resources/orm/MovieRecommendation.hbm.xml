<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.sjsu.cmpe.datamining.model.MovieRecommendation" table="movie_reco" catalog="cmpe_datamining">
    	<id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
    	<property name="movieId" type="java.lang.Integer">
            <column name="movie_id"/>
        </property>
        <property name="recoMovieId" type="java.lang.Integer">
            <column name="reco_movie_id"/>
        </property>
    </class>
</hibernate-mapping>