<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.sjsu.cmpe.datamining.model.UserMovies" table="user_movies" catalog="cmpe_datamining">
    	<id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
    	<property name="username" type="string">
            <column name="username" length="45" />
        </property>
        <property name="movieId" type="java.lang.Integer">
            <column name="movie_id"/>
        </property>
        <property name="watchedOn" type="java.sql.Timestamp">
            <column name="watched_on"/>
        </property>
    </class>
</hibernate-mapping>